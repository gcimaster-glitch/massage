
{
  "name": "soothe-care-cube-jp",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:sandbox": "wrangler pages dev dist --ip 0.0.0.0 --port 3000",
    "dev:full": "wrangler pages dev dist --d1=soothe-db-production --local --ip 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "wrangler pages dev dist",
    "deploy": "npm run build && wrangler pages deploy dist --project-name soothe-care-cube-jp",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name soothe-care-cube-jp --branch main",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:create": "wrangler d1 create soothe-db-production",
    "db:migrate:local": "wrangler d1 migrations apply soothe-db-production --local",
    "db:migrate:prod": "wrangler d1 migrations apply soothe-db-production",
    "db:seed": "wrangler d1 execute soothe-db-production --local --file=./seed.sql",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && npm run db:migrate:local && npm run db:seed",
    "db:console:local": "wrangler d1 execute soothe-db-production --local",
    "db:console:prod": "wrangler d1 execute soothe-db-production",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true",
    "test:api": "curl http://localhost:3000/api/health"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "hono": "^4.0.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.561.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250705.0",
    "@hono/vite-cloudflare-pages": "^0.4.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "wrangler": "^3.100.0"
  }
}
